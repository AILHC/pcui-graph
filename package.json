    "watch": "ENVIRONMENT=development webpack --config webpack.config.js --watch",
    "build:prod": "webpack --config webpack.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
