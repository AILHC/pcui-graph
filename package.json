  "scripts": {
    "build_": "cd node_modules/@playcanvas/pcui; npm install; EXTRACT_CSS=true npm run build;cp dist/pcui-binding.js ../../../src/pcui-binding-external.js; cp dist/pcui.js ../../../src/pcui-external.js; cd ../../../; webpack --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint --ext .js src",
    "storybook": "jsdoc -r -X src/components > ./.storybook/utils/jsdoc-ast.json && start-storybook -p 9010 -s public",
